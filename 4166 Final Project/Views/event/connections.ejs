<%- include('../partials/header.ejs')%>      
      <div class="LCont">
        <div class="displayEvents">
            <% if(events.length){ %>
                
                <%categories.forEach(category=>{%>
                <h1><%=category%></h1>
                    <%events.forEach(event=>{%>
                        <% if(event.category === category){ %>
                        <a href = "/events/<%=event.id%>"> <p><%= event.title %></p></a>
                        <%}%>
                    <%})%>
               
                <%})%>
           
            <%} else{%><p>There are no connections to display</p><%}%>
        </div>
      </div>
      <div class="CCont">
      </div>  
      <div class="RCont"></div>
